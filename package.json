    "@typescript-eslint/parser": "^5.54.0",
    "eslint": "^8.35.0",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.1",
    "ts-node": "^10.7.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "path": "^0.12.7",
    "uuid": "^9.0.0",
    "yargs": "^17.6.2"
